{
  "name": "vottuscode/hcaptcha",
  "description": "hCaptcha for Nette Forms",
  "keywords": ["nette", "forms", "captcha", "hcaptcha", "recaptcha"],
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/VottusCode/hcaptcha",
  "authors": [
    {
      "name": "Milan Felix Å ulc",
      "homepage": "https://f3l1x.io"
    },
    {
      "name": "VottusCode",
      "homepage": "https://github.com/vottuscode"
    }
  ],
  "suggest": {
    "ext-openssl": "To make requests via https"
  },
  "require": {
    "php": "^8.1",
    "nette/di": "*",
    "nette/forms": "*",
    "nette/utils": "*"
  },
  "require-dev": {
    "phpstan/extension-installer": "*",
    "phpstan/phpstan": "*",
    "phpstan/phpstan-doctrine": "*",
    "phpstan/phpstan-nette": "*",
    "phpstan/phpstan-phpunit": "*",
    "phpstan/phpstan-symfony": "*"
  },
  "autoload": {
    "psr-4": {
      "Contributte\\HCaptcha\\": "src"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "phpstan/extension-installer": true
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "3.4.x-dev"
    }
  }
}
