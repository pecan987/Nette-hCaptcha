{
  "name": "vottuscode/hcaptcha",
  "description": "hCaptcha for Nette Forms",
  "keywords": ["nette", "forms", "captcha", "hcaptcha", "recaptcha"],
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/VottusCode/hcaptcha",
  "authors": [
    {
      "name": "Milan Felix Å ulc",
      "homepage": "https://f3l1x.io"
    },
    {
      "name": "VottusCode",
      "homepage": "https://github.com/vottuscode"
    }
  ],
  "suggest": {
    "ext-openssl": "To make requests via https"
  },
  "require": {
    "php": "^7.2",
    "nette/di": "~3.0.0",
    "nette/forms": "~3.0.0",
    "nette/utils": "~3.0.0|~3.1.0"
  },
  "require-dev": {
    "ninjify/nunjuck": "^0.3.0",
    "ninjify/qa": "^0.9.0",
    "phpstan/phpstan-deprecation-rules": "^0.11",
    "phpstan/phpstan-nette": "^0.11",
    "phpstan/phpstan-shim": "^0.11",
    "phpstan/phpstan-strict-rules": "^0.11"
  },
  "autoload": {
    "psr-4": {
      "Contributte\\ReCaptcha\\": "src"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "3.4.x-dev"
    }
  }
}
